<%
  @title ||= :ARTICLES.t
  tabs = index_tabs + create_links(@links)
  @tabsets = { right: draw_tab_set(tabs) }

  flash_error(@error) if @error && @objects.empty?
%>

<!-- list the articles -->
<%= paginate_block(@pages) do %>
  <div class="list-group")>
    <%= render partial: "articles/object", collection: @objects %>
  </div>
<% end %>
