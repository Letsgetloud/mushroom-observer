<%
  @title ||= :list_projects_title.t

  tabs = [
    link_to(:list_projects_add_project.t, new_project_path(q: get_query_param))
  ]
  tabs += create_links(@links)
  @tabsets = { right: draw_tab_set(tabs) }

  flash_error(@error) if @error && @objects.empty?
%>

<!-- list the projects -->
<%= paginate_block(@pages) do %>
  <div class="card")>
    <%= render(partial: "object", collection: @objects) %>
  </div>
<% end %>
